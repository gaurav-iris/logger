.bc{border: 1px solid #ddd;color: #888;margin:15px auto;clear: both; width:100%;background:#f5f5f5;}
.bc ul > li:after {border-bottom: 1px solid #ddd;border-right: 1px solid #ddd;content: ""; display: block;height: 26px;position: absolute;right: -5px;top: -5px;transform: rotate(-45deg);width: 26px;}
.bc ul{padding: 10px 0;}
*:before, *:after {box-sizing: border-box;}
.bc ul > li {padding: 0 20px;position: relative;white-space: nowrap;}
.bc ul > li {display: inline-block;}
.bc ul > li + li:before {content: "";padding: 0;}
.bc ul > li + li:before {color: #fff;content: " ";padding: 0 5px;}
.bc ul > li {padding: 0 10px;position: relative;white-space: nowrap;}
.bc ul > li {display: inline-block;}
.bc ul li a {color: #888; font-size: 13px;margin: 0 5px;}
.bc ul li a:hover {color: #000;}
.bc ul li ._a{color: #000;}


