.act .radio{ margin-bottom: 20px;}
.act .radio label {display: inline-block; cursor: pointer;position: relative;padding-left: 25px;margin-right: 15px;font-size: 13px;}  
.act .radio input[type=radio] { display: none;}  
.act .radio label:before {content: "";display: inline-block; width: 16px;height: 16px; margin-right: 10px; position: absolute; left: 0;bottombottom: 1px;background-color: #1e8bc3;box-shadow: inset 0px 2px 3px 0px rgba(0, 0, 0, .3), 0px 1px 0px 0px rgba(255, 255, 255, .8);}  
.act .radio label:before {border-radius: 8px;}
.act input[type=radio]:checked + label:before {content: "\2022";color: #f3f3f3; font-size: 30px;  text-align: center;  line-height: 18px;  } 
.col3 .null{ position: relative; text-align: center; background: none repeat scroll 0 0 rgba(0, 0, 0, 0.03); width: 100%; height: 100%;}
.col3 .null h3{position: absolute; top: 55%;color: #888; font-size: 16px; left: 32%;margin: 0;padding: 0;}
.col3 .null i {bottom:42%;color: #b5b5b5;font-size:36px; height: 48px;left: 50%;margin-left: -24px;position: absolute;width: 48px;}
.col3 .null .te{background:#b5b5b5;color: #ffffff;font-size: 13px;font-weight: bold;left: 0;padding: 4px 10px;position: absolute;top: 10px;}
