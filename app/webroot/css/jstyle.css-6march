.fileUpload {
	position: relative;
	overflow: hidden;
	font-family: "Open Sans",sans-serif;
}
.fileUpload input.uploadme{
	position: absolute;
	top: 0;
	left: 0;
	margin: 0;
	padding: 0;
	font-size: 20px;
	cursor: pointer;
	opacity: 0;
	filter: alpha(opacity=0);
}
.fileUpload .xl{ margin-left: 0;}
.fileUpload span{ width: auto !important;margin-left: 0 !important;}
.fileList{margin: 10px 0;}
